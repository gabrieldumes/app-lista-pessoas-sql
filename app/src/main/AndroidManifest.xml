<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.listadepessoassql">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_app_lista_pessoas"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_app_lista_pessoas_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ListaDePessoasSQL">
        <activity android:name=".activity.FiltrarActivity"></activity>
        <activity android:name=".activity.InserirPessoaActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>